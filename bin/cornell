#!/usr/bin/env node

var program = require('commander');
var lib     = require('../lib/common');

lib.notifyUpdate();

program
  .command('class [search]', 'search for class info')
  .command('eat [hall]',     'display menus for hall')
  .command('netid [search]', 'get people\'s netid or name')
  // .command('hours [hall]', 'show hours for dining hall')
  // .command('print [printer] [document]', 'show hours for dining hall')

program.parse(process.argv);

